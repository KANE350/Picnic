package com.kgr.picnic;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {

		Scanner in = new Scanner(System.in);
		int n = in.nextInt();
			
		
		if(n>=1) {
			for (int i = 1; i <= 9; i++) {
				System.out.println(n + "*" + i + "=" + (n * i));
			}
		}


	}
}
